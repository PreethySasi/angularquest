{
  "name": "angular-quest-server",
  "description": "Server side Api for the Angular Quest project",
  "author": "wagner.alves@version1.com",
  "repository": "https://www.github.com/keyrox/angularquest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "ts -p src",
    "node-run": "node dist/index.js",
    "build": "tsc -p src",
    "dev": "concurrently \"tsc -p src\" \"nodemon dist/index.js\"",
    "serve": "concurrently \"tsc -p src\" \"node dist/index.js\"",
    "debug": "concurrently \"tsc -w -p src\" \"nodemon --inspect dist/index.js\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts"
  },
  "dependencies": {
    "@types/elasticsearch": "^5.0.24",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.110",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.9",
    "body-parser": "~1.13.2",
    "concurrently": "^3.6.0",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "~2.2.0",
    "elasticsearch": "^15.1.1",
    "express": "~4.16.3",
    "glob": "^5.0.14",
    "jade": "~1.11.0",
    "lodash": "^3.10.1",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "mongoose": "^4.11.1",
    "morgan": "~1.6.1",
    "nodemon": "^1.17.5",
    "passport": "^0.3.2",
    "q": "^1.4.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.1",
    "typescript": "^2.9.2",
    "typescript-ioc": "^1.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.4",
    "@types/sinon": "^4.3.3",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3"
  }
}
